@model Rehleti.Models.ListOfDatesForChalet

@{
    ViewData["Title"] = "Details";
}
<br />
<br />
<h1>Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Check-In
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateFrom)
        </dd>
        <dt class = "col-sm-2">
            Check-Out
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateTo)
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Chalet)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Chalet.Name)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-outline-info" asp-controller="AccountInfo" asp-action="YourChalet">Back to List</a>
</div>
